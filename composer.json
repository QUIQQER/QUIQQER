  "repositories": [
    {
      "type": "composer",
      "url": "https://update.quiqqer.com"
    }
  ],
  "require": {
    "php": ">=5.3",
    "composer/composer": "^1.1.0",
    "tedivm/stash": "0.14.*",
    "robloach/component-installer": "0.0.12",
    "phpmailer/phpmailer": "v5.2.*",
    "symfony/http-foundation": "2.6.*",
    "symfony/console": "2.5",
    "html2text/html2text": "1.0.1",
    "intervention/image": "2.2.*",
    "t7systems/blowfish": "1.0.0",
    "pcsg\/composer-assets": "*",
    "quiqqer/composer": "1.*",
    "quiqqer/qui": ">=0.13",
    "quiqqer/qui-php": "*0.10.0",
    "quiqqer/smarty3": "1.*",
    "quiqqer/ckeditor4": "1.*",
    "quiqqer/calendar-controls": "1.*",
    "quiqqer/countries": "1.*",
    "quiqqer/colorpicker": ">=1.1",
    "quiqqer/translator": "1.*",
    "quiqqer/utils": ">=1.9",
    "quiqqer/cron": "1.*",
    "quiqqer/log": "*1.*",
    "quiqqer/diashow": "*>=0.9",
    "quiqqer/lockclient": ">=0.9",
    "quiqqer/messages": "1.*",
    "quiqqer/meta": "0.9.*",
    "npm-asset/intl": "1.*",
    "npm-asset/nouislider": "*",
    "npm-asset/mustache": "2.*",
    "npm-asset/urijs": "1.*"
  },
  "require-dev": {
    "mikey179/vfsStream": "1.*"
  },
  "scripts": {
    "post-install-cmd": [
      "QUI\\Update::onInstall"
    ],
    "post-update-cmd": [
      "QUI\\Update::onUpdate"
    ]
  },
  "autoload": {
    "psr-0": {
      "QUI": "lib/"
    }
  }
}
