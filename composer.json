{
  "name": "quiqqer/quiqqer",
  "type": "quiqqer-system",
  "description": "A modular based management system written in JavaScript and PHP",
  "version": "dev-master",
  "license": "GPL-3.0+",
  "authors": [
    {
      "name": "Henning Leutz",
      "email": "leutz@pcsg.de",
      "homepage": "http://www.pcsg.de",
      "role": "Developer"
    },
    {
      "name": "Moritz Scholz",
